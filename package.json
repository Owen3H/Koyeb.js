{
  "name": "koyeb.js",
  "version": "4.1.0",
  "description": "An unofficial wrapper for interacting with the Koyeb API.",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.cjs"
  },
  "types": "dist/types.d.ts",
  "type": "module",
  "scripts": {
    "test": "cd test-pkg & npm test",
    "build": "rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir dist/esm",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "files": [
    "dist"
  ],
  "funding": "https://github.com/sponsors/Owen3H",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Owen3H/Koyeb.js.git"
  },
  "keywords": [
    "koyeb",
    "koyeb-api",
    "koyeb-wrapper",
    "api-wrapper",
    "wrapper"
  ],
  "author": "Owen77Stubbs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Owen3H/Koyeb.js/issues"
  },
  "homepage": "https://github.com/Owen3H/Koyeb.js#readme",
  "dependencies": {
    "tslib": "^2.6.2",
    "undici": "^5.24.0",
    "ws": "^8.14.1"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.6.0",
    "@types/ws": "^8.5.5",
    "eslint": "^8.49.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.1",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4"
  }
}
