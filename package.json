{
  "name": "koyeb.js",
  "version": "4.0.0",
  "description": "An unofficial wrapper for interacting with the Koyeb API.",
  "main": "dist/cjs/koyeb.js",
  "module": "dist/esm/koyeb.js",
  "type": "module",
  "scripts": {
    "test": "cd test-pkg & npm test",
    "build": "rmdir dist /S /Q && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --outDir dist/esm && npm run dets:esm",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs && npm run dets:cjs"
  },
  "files": [
    "dist"
  ],
  "funding": "https://github.com/sponsors/Owen3H",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Owen3H/Koyeb.js.git"
  },
  "keywords": [
    "koyeb",
    "koyeb-api",
    "koyeb-wrapper",
    "api-wrapper",
    "wrapper"
  ],
  "author": "Owen77Stubbs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Owen3H/Koyeb.js/issues"
  },
  "homepage": "https://github.com/Owen3H/Koyeb.js#readme",
  "dependencies": {
    "tslib": "^2.5.3",
    "undici": "^5.14.0",
    "ws": "^8.11.0"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/ws": "^8.5.4",
    "typescript": "^5.1.3"
  }
}
